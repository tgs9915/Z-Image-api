# Z-Image API 配置
ZIMAGE_API_URL=https://mrfakename-z-image-turbo.hf.space

# 认证配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
CONSOLE_USERNAME=admin
CONSOLE_PASSWORD=zimage@2024

# API 密钥 (逗号分隔)
API_KEYS=sk-zimage-test-key-001,sk-zimage-test-key-002

# 公开 URL (部署后填写，例如 https://your-app.vercel.app)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# 代理池配置
PROXY_POOL_ENABLED=true
PROXY_POOL_MAX_DAILY=5
PROXY_POOL_UPDATE_INTERVAL=300
PROXY_HEALTH_CHECK_INTERVAL=120
PROXY_PROMOTE_THRESHOLD=5.0
PROXY_DEMOTE_FAIL_COUNT=3

# 代理源列表 (可选，逗号分隔，留空则使用默认源)
